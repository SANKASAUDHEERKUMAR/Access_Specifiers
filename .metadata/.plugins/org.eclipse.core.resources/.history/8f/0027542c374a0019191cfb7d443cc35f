package javaPrograms;

class parent{
	void parentmethod(){
	System.out.println("parent class Executed");
	}
}
class child extends parent{
	void childmethod(){
		System.out.println("Child class Executed");
		}	
}
public class Inheritance {

	public static void main(String[] args) {
		child c=new child();
		c.parentmethod();
		c.childmethod();
	}

}
